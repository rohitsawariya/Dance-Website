    .section3 
         div.ctct
             h1 Register Here
         div.frm 
            form(action="/contact" method="post")
                input(type="text" ,name="name", id="name", placeholder="ENTER YOUR NAME")
                //- label(for="email") E-mail:
                input(type="email" name="email" id="email" placeholder="WRITE YOUR E-MAIL")
                //- label(for="gender") Select Gender:
                select(name="gender" id="gender")
                     option(value="default") Select
                     option(value="Male") Male
                     option(value="Female") Female
                     option(value="other") Other
                //- label(for="number") Mobile-No:
                input(type="tel" name="number" id="number" placeholder="ENTER YOUR MOBILE")
                //- label(for="text") Tell About You..
                textarea(name="text" placeholder="More About You.." id="text" cols="45" rows="10")
                button.btn(type="submit") SUBMIT
 
